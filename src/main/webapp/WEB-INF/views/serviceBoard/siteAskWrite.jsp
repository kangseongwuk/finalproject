          <form:input path="m_name" class="form-control mb-3" id="name" name="name" placeholder="이름" />
          <form:errors path="m_name" style="color:red"></form:errors>
          <form:input path="m_id" class="form-control mb-3" id="id" name="id" placeholder="아이디" />
          <form:errors path="m_id" style="color:red"></form:errors>
          <form:input path="sa_title" class="form-control mb-3" id="subject" name="subject" placeholder="제목" />
          <form:errors path="sa_title" style="color:red"></form:errors>
          <form:textarea path="sa_contents" name="message" id="message" class="form-control mb-3" placeholder="문의내용" ></form:textarea>
          <form:errors path="sa_contents" style="color:red"></form:errors>
          <form:input type="file" path="upload_file" accept="image/*" class="form-control mb-3" id="subject" name="subject" placeholder="첨부내용" />
